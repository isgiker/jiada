        <script type="text/javascript">
            $('#mallNav').scrollToFixed();
            $(document).ready(function(){
                //图片轮播
                $('#flash1').slideBox({
                        duration : 0.3,//滚动持续时间，单位：秒
                        easing : 'linear',//swing,linear//滚动特效
                        delay : 3,//滚动延迟时间，单位：秒
                        hideClickBar : false,//不自动隐藏点选按键
                        clickBarRadius : 10
                });
                //Tabs内容切换 
                $(".marketRight_mod_news_tab").mouseover(function() {
                    var $this = $(this),tbcontentid = $this.data('tbcontentid');
                    $this.parent().parent().find(".content").hide();
                    $('#marketRight_mod_news_tab_content_'+tbcontentid).show();
                    $this.prevAll().removeClass('cur');
                    $this.nextAll().removeClass('cur');
                    //为当前元素添加class
                    $this.addClass("cur");
                });
                //floor 切换分类内容
                $(".cname").mouseover(function(){
                    var $this = $(this),
			id = $this.data('id'),
                        floor = $this.parent().parent().parent().parent().data('floor');
                    //隐藏当前元素同级的所有元素内容
                    $("#"+id).prevAll().hide();
                    $("#"+id).nextAll().hide();
                    //显示当前元素下的内容
                    if($("#"+id).length >0){
                        $("#"+id).show();
                    }
                    //删除当前元素的所以同级元素的class
                    $this.prevAll().removeClass('curr_category_nav_'+floor);
                    $this.nextAll().removeClass('curr_category_nav_'+floor);
                    //为当前元素添加class
                    $this.addClass("curr_category_nav_"+floor);
                        
                });
                
                //floor 切换分类内容--热销商品
                $(".goods_small_one").mouseover(function() {
                    var $this = $(this);
                    $this.parent().parent().find(".goods_big_one").hide();
                    $this.prev().show();
                    $this.parent().parent().find(".goods_small_one").show();
                    $this.hide();
                });
                
                
                
            });
        </script>
